{
  "name": "ng-builder-google-maps",
  "version": "1.1.1",
  "description": "Angular CLI builder that adds the Google Maps API into your application",
  "keywords": [
    "angular",
    "google",
    "maps"
  ],
  "bugs": {
    "url": "https://github.com/Developer-Plexscape/ng-builder-google-maps/issues"
  },
  "homepage": "https://github.com/Developer-Plexscape/ng-builder-google-maps#readme",
  "main": "dist/index.js",
  "builders": "builders.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "scripts": {
    "build": "tsc && npm run build:schematics",
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "copy:schema": "cp src/schema.json dist/schema.json",
    "copy:schematics": "cp schematics/*.json dist/schematics",
    "postbuild": "npm run copy:schema && npm run copy:schematics"
  },
  "devDependencies": {
    "@types/jasmine": "~4.3.2",
    "@types/node": "^20.2.5",
    "jasmine": "^5.0.0",
    "jasmine-node": "^3.0.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@angular-devkit/architect": "0.1600.3",
    "@angular-devkit/schematics": "16.0.3"
  },
  "schematics": "./dist/schematics/collection.json"
}
